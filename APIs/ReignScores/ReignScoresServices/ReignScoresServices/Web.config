<configuration>
	<system.web>
		<compilation debug="true" targetFramework="4.5.1" />
		<httpRuntime targetFramework="4.5.1" />
		<!--<customErrors mode="Off"/>-->
	</system.web>

	<appSettings>
		<add key="webpages:Enabled" value="true" />
	</appSettings>

	<location path="Services">
		<system.webServer>
			<directoryBrowse enabled="false" />
		</system.webServer>
	</location>
</configuration>